    <validationRules>
        <fullName>Rule_1_OrderDate</fullName>
        <active>true</active>
        <errorConditionFormula>Order__c &lt;  TODAY()</errorConditionFormula>
        <errorDisplayField>Order__c</errorDisplayField>
        <errorMessage>Order date can&apos;t be prior to current date!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Rule_2_Phonefield</fullName>
        <active>true</active>
        <errorConditionFormula>OR( 
   Not(ISNUMBER( Phone__c )),
   (LEN( Phone__c )&lt; 10  || LEN( Phone__c )&gt; 10)   
)</errorConditionFormula>
        <errorDisplayField>Phone__c</errorDisplayField>
        <errorMessage>Phone field must contain number and that too of 10 digits!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Rule_3_GSTNumber</fullName>
        <active>true</active>
        <errorConditionFormula>AND(

     Total_Price__c &gt; 2000,
     ISBLANK( GST_Number__c )

)</errorConditionFormula>
        <errorDisplayField>GST_Number__c</errorDisplayField>
        <errorMessage>For the total order price above 2000, GST number must be populated!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Rule_4_Status</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  
  ISPICKVAL( PRIORVALUE(Status__c), &apos;Delivered&apos;),
  Not( ISPICKVAL(Status__c, &apos;Delivered&apos;) )

)</errorConditionFormula>
        <errorMessage>Status of order canâ€™t be changed to any other status once it is set
to be delivered!</errorMessage>
    </validationRules>
